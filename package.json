{
	"name": "hexdump-viewer",
	"displayName": "hexdump-viewer",
	"description": "Extension to view hexdump from a trace in a more usable way.",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.39.0"
	},
	"publisher": "zcee",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "hexcontent",
				"extensions": [
					".hexcontent"
				]
			}
		],
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "View selected hex..."
			},
			{
				"command": "hexcontent.switch",
				"title": "Switch ASCII/EBCDIC..."
			}
		],
		"grammars": [
			{
				"language": "hexcontent",
				"scopeName": "source.hexcontent",
				"path": "./syntaxes/hexdump.tmLanguage"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "hexcontent.switch",
					"when": "editorLangId == hexcontent"
				}
			],
			"commandPalette": [
				{
					"command": "extension.helloWorld",
					"when": "editorHasSelection"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"codepage": "^1.14.0",
		"hexy": "^0.3.0"
	}
}
