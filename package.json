{
	"name": "hexdump-viewer",
	"displayName": "hexdump-viewer",
	"description": "Extension to view hexdump from a trace in a more usable way.",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.39.0"
	},
	"publisher": "crshnburn",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hexcontent.viewHex"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "hexcontent",
				"extensions": [
					".hexcontent"
				]
			}
		],
		"commands": [
			{
				"command": "hexcontent.viewHex",
				"title": "View selected hex..."
			},
			{
				"command": "hexcontent.switch",
				"title": "Switch ASCII/EBCDIC..."
			}
		],
		"grammars": [
			{
				"language": "hexcontent",
				"scopeName": "source.hexcontent",
				"path": "./syntaxes/hexdump.tmLanguage"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "hexcontent.switch",
					"when": "editorLangId == hexcontent"
				}
			],
			"commandPalette": [
				{
					"command": "hexcontent.viewHex",
					"when": "editorHasSelection"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.28",
		"@types/vscode": "^1.48.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.1",
		"tslint": "^5.20.1",
		"typescript": "^3.9.7",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@crshnburn/hexyz": "^1.0.2",
		"codepage": "^1.14.0"
	}
}
